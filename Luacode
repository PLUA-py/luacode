-- Comprehensive Script: Kill, CT (Tsunami, Fatality, c00lkidd+Skin+Immortality), Teleport, Currency Editor, Anti-Kick
-- Большое UI с разделами по порядку: Kill, CT, Teleport, Currency, AntiKick

local Players       = game:GetService("Players")
local RunService    = game:GetService("RunService")
local TweenService  = game:GetService("TweenService")
local UserInput     = game:GetService("UserInputService")
local LocalPlayer   = Players.LocalPlayer
local Camera        = workspace.CurrentCamera

-- Удаляем старый GUI, если есть
local existingGUI = LocalPlayer:FindFirstChild("BigAdminGUI")
if existingGUI then existingGUI:Destroy() end

-- === Основной ScreenGui ===
local screenGui = Instance.new("ScreenGui", LocalPlayer:WaitForChild("PlayerGui"))
screenGui.Name = "BigAdminGUI"

local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Size = UDim2.new(0, 300, 0, 480)
mainFrame.Position = UDim2.new(0.5, -150, 0.5, -240)
mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
mainFrame.BackgroundTransparency = 0.2
mainFrame.BorderSizePixel = 0
mainFrame.Active = true
mainFrame.Draggable = true

local title = Instance.new("TextLabel", mainFrame)
title.Size = UDim2.new(1, 0, 0, 30)
title.Position = UDim2.new(0, 0, 0, 0)
title.BackgroundTransparency = 1
title.Text = "BigAdmin GUI"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.GothamBold
title.TextSize = 18

-- === Список разделов (Tabs) ===
local tabsFolder = Instance.new("Folder", mainFrame)
tabsFolder.Name = "Tabs"

local function makeTabButton(text, y)
    local btn = Instance.new("TextButton", mainFrame)
    btn.Size = UDim2.new(0, 100, 0, 30)
    btn.Position = UDim2.new(0, 0, 0, y)
    btn.Text = text
    btn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    btn.TextColor3 = Color3.fromRGB(255, 255, 255)
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 14
    return btn
end

local btnKill     = makeTabButton("Kill", 40)
local btnCT       = makeTabButton("CT", 80)
local btnTeleport = makeTabButton("Teleport", 120)
local btnCurr     = makeTabButton("Currency", 160)
local btnAntiKick = makeTabButton("AntiKick", 200)

-- === Контейнеры для контента ===
local function makeContentFrame()
    local fr = Instance.new("Frame", mainFrame)
    fr.Size = UDim2.new(1, -110, 1, -240)
    fr.Position = UDim2.new(0, 110, 0, 40)
    fr.BackgroundTransparency = 1
    fr.Visible = false
    return fr
end

local contentKill     = makeContentFrame()
local contentCT       = makeContentFrame()
local contentTeleport = makeContentFrame()
local contentCurr     = makeContentFrame()
local contentAntiKick = makeContentFrame()

-- Показать нужный фрейм, скрыть остальные
local function showFrame(target)
    for _, frame in pairs({contentKill, contentCT, contentTeleport, contentCurr, contentAntiKick}) do
        frame.Visible = (frame == target)
    end
end

btnKill.MouseButton1Click:Connect(function() showFrame(contentKill) end)
btnCT.MouseButton1Click:Connect(function() showFrame(contentCT) end)
btnTeleport.MouseButton1Click:Connect(function() showFrame(contentTeleport) end)
btnCurr.MouseButton1Click:Connect(function() showFrame(contentCurr) end)
btnAntiKick.MouseButton1Click:Connect(function() showFrame(contentAntiKick) end)

-- Сразу открываем Kill
showFrame(contentKill)

-- ====================================================
-- =============== 1. КILL (Убить игрока) ==============
-- ====================================================
local lblKill = Instance.new("TextLabel", contentKill)
lblKill.Size = UDim2.new(1, -20, 0, 20)
lblKill.Position = UDim2.new(0, 10, 0, 10)
lblKill.BackgroundTransparency = 1
lblKill.Text = "Kill Player by Name:"
lblKill.TextColor3 = Color3.fromRGB(255, 255, 255)
lblKill.Font = Enum.Font.Gotham
lblKill.TextSize = 14

local txtKillName = Instance.new("TextBox", contentKill)
txtKillName.Size = UDim2.new(1, -20, 0, 30)
txtKillName.Position = UDim2.new(0, 10, 0, 40)
txtKillName.PlaceholderText = "Введите имя игрока"
txtKillName.TextColor3 = Color3.fromRGB(0, 0, 0)
txtKillName.Font = Enum.Font.Gotham
txtKillName.TextSize = 14

local btnKillExec = Instance.new("TextButton", contentKill)
btnKillExec.Size = UDim2.new(1, -20, 0, 30)
btnKillExec.Position = UDim2.new(0, 10, 0, 80)
btnKillExec.Text = "Kill"
btnKillExec.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
btnKillExec.TextColor3 = Color3.fromRGB(255, 255, 255)
btnKillExec.Font = Enum.Font.Gotham
btnKillExec.TextSize = 14

btnKillExec.MouseButton1Click:Connect(function()
    local targetName = txtKillName.Text
    if targetName and targetName ~= "" then
        for _, pl in pairs(Players:GetPlayers()) do
            if pl.Name:lower() == targetName:lower() and pl.Character and pl.Character:FindFirstChildOfClass("Humanoid") then
                pl.Character:FindFirstChildOfClass("Humanoid"):TakeDamage(1e9)
                break
            end
        end
    end
end)

