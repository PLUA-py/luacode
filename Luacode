local lblCurr = Instance.new("TextLabel", contentCurr)
lblCurr.Size = UDim2.new(1, -20, 0, 20)
lblCurr.Position = UDim2.new(0, 10, 0, 10)
lblCurr.BackgroundTransparency = 1
lblCurr.Text = "Set Value for Leaderstats Currency:"
lblCurr.TextColor3 = Color3.fromRGB(255, 255, 255)
lblCurr.Font = Enum.Font.Gotham
lblCurr.TextSize = 14

local txtCurrValue = Instance.new("TextBox", contentCurr)
txtCurrValue.Size = UDim2.new(1, -20, 0, 30)
txtCurrValue.Position = UDim2.new(0, 10, 0, 40)
txtCurrValue.PlaceholderText = "0 - 10000"
txtCurrValue.TextColor3 = Color3.fromRGB(0, 0, 0)
txtCurrValue.Font = Enum.Font.Gotham
txtCurrValue.TextSize = 14
txtCurrValue.Text = ""

local btnSetCurr = Instance.new("TextButton", contentCurr)
btnSetCurr.Size = UDim2.new(1, -20, 0, 30)
btnSetCurr.Position = UDim2.new(0, 10, 0, 80)
btnSetCurr.Text = "Set Currency"
btnSetCurr.BackgroundColor3 = Color3.fromRGB(200, 100, 0)
btnSetCurr.TextColor3 = Color3.fromRGB(255, 255, 255)
btnSetCurr.Font = Enum.Font.Gotham
btnSetCurr.TextSize = 14

btnSetCurr.MouseButton1Click:Connect(function()
    local val = tonumber(txtCurrValue.Text)
    if val then
        local ls = LocalPlayer:FindFirstChild("leaderstats") or LocalPlayer:FindFirstChild("Leaderstats")
        if ls then
            for _, stat in pairs(ls:GetChildren()) do
                if stat:IsA("IntValue") or stat:IsA("NumberValue") then
                    stat.Value = math.clamp(val, 0, 10000)
                end
            end
        end
    end
end)

