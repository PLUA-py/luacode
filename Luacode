lblTeleport.TextColor3 = Color3.fromRGB(255, 255, 255)
lblTeleport.Font = Enum.Font.Gotham
lblTeleport.TextSize = 14

-- Toggle Teleport
local teleportEnabled = false
local btnTeleportToggle = Instance.new("TextButton", contentTeleport)
btnTeleportToggle.Size = UDim2.new(1, -20, 0, 30)
btnTeleportToggle.Position = UDim2.new(0, 10, 0, 50)
btnTeleportToggle.Text = "Teleport: Off"
btnTeleportToggle.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
btnTeleportToggle.TextColor3 = Color3.fromRGB(255, 255, 255)
btnTeleportToggle.Font = Enum.Font.Gotham
btnTeleportToggle.TextSize = 14

btnTeleportToggle.MouseButton1Click:Connect(function()
    teleportEnabled = not teleportEnabled
    btnTeleportToggle.Text = "Teleport: " .. (teleportEnabled and "On" or "Off")
    btnTeleportToggle.BackgroundColor3 = teleportEnabled and Color3.fromRGB(50, 150, 50) or Color3.fromRGB(100, 100, 100)
end)

-- Обработка клика/тача для телепорта
UserInput.InputBegan:Connect(function(input, gp)
    if gp then return end
    if teleportEnabled then
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            local pos = Vector3.new()
            -- Получаем позицию курсора в мире
            local unitRay = Camera:ScreenPointToRay(input.Position.X, input.Position.Y)
            local ray = Ray.new(unitRay.Origin, unitRay.Direction * 999)
            local hit, hitPos = workspace:FindPartOnRay(ray, LocalPlayer.Character)
            if hitPos then
                pos = hitPos + Vector3.new(0, 3, 0)
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(pos)
                end
            end
        end
    end
end)

-- Я сделал код

